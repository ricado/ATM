<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" 
"http://tuckey.org/res/dtds/urlrewrite2.6.dtd"> 
<urlrewrite> 
<rule> 
<from>^/essay/(\d+)\.html$</from> 
<to type="forward">/essay_detail.action?essayId=$1</to> 
</rule> 

<rule> 
<from>^/clickGood/(\d+)\.html$</from> 
<to type="forward">/essay_clickGoodPeople.action?essayId=$1</to> 
</rule>

<rule> 
<from>^/moreClickGood/(\d+)/(\d+)\.html$</from> 
<to type="forward">/essay_moreClickGoodPeople.action?essayId=$1&amp;first=$2</to> 
</rule>

<rule> 
<from>^/replyList/(\d+)\.html$</from> 
<to type="forward">/replyList.jsp?essayId=$1</to> 
</rule>

<rule> 
<from>^/recuit/(\d+)\.html$</from> 
<to type="forward">/recuit_detail.action?reInfoId=$1</to>
</rule>
<rule> 
<from>^/apply/(\d+)\.html$</from> 
<to type="forward">/apply_detail.action?apInfoId=$1</to>
</rule>

</urlrewrite>